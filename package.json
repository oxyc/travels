{
  "name": "travels",
  "version": "1.0.0",
  "description": "A record of my travels",
  "main": "index.js",
  "scripts": {
    "test": "xo && node scripts/lint-country.js _data/countries/*.json && geojsonhint _data/*/*.json && jsonlint -q *.json _data/*/*.json && jshint _data/*/*.json",
    "build": "for i in _data/trips/*.json; do n=\"$(basename $i)\"; topojson -p -o geojson/trips/${n%.*}.topojson $i; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxyc/travels.git"
  },
  "author": {
    "name": "Oskar Schöldström",
    "email": "public@oxy.fi",
    "url": "http://whatareyoudoinghere.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oxyc/travels/issues"
  },
  "homepage": "https://github.com/oxyc/travels#readme",
  "devDependencies": {
    "express": "^4.13.3",
    "geojsonhint": "^1.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-inline-image": "0.0.4",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.4.1",
    "jshint": "^2.8.0",
    "jsonlint": "^1.6.2",
    "xo": "^0.8.0"
  },
  "xo": {
    "space": true,
    "globals": [
      "jQuery",
      "L",
      "_",
      "Fraction"
    ],
    "ignores": [
      "_site/**"
    ],
    "envs": [
      "browser"
    ]
  },
  "dependencies": {
    "file-exists": "^0.1.1",
    "geojson-merge": "^0.1.0",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "topojson": "^1.6.19"
  }
}
